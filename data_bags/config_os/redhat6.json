{
  "id": "redhat6",
  "doc": "Distribution and version specific provisioning for Red Hat 6",
  "repo_packages": {
    "rpmfusion-free-release-6": {
      "source": "https://mirrors.rpmfusion.org/free/el/rpmfusion-free-release-6.noarch.rpm",
      "filename": "rpmfusion-free-release-6.noarch.rpm"
    },
    "rpmfusion-nonfree-release-6": {
      "source": "https://mirrors.rpmfusion.org/nonfree/el/rpmfusion-nonfree-release-6.noarch.rpm",
      "filename": "rpmfusion-nonfree-release-6.noarch.rpm"
    }
  },
  "repo_config_commands": {
  },
  "default_packages": [
    "vim", "atop", "htop", "nc", "tree", "wget", "policycoreutils-python"
  ],
  "default_scripts": {
    "/etc": {
      "bashrc": {
        "subdir": "etc",
        "owner": "root",
        "group": "root",
        "mode": "0755"
      }
    },
    "/usr/local/etc": {
      "shell-basics": {
        "subdir": "usr/local/etc",
        "owner": "root",
        "group": "root",
        "mode": "0755"
      },
      "shell-history": {
        "subdir": "usr/local/etc",
        "owner": "root",
        "group": "root",
        "mode": "0755"
      }
    }
  }
}
